<%= render "content/teaser", content: material do %>
  <div class="material-name small-6 medium-6 large-6 columns">
    <%= link_to material.name, material %>
  </div>

  <div class="material-image small-6 medium-6 large-6 columns">
    <div class="material-preview">
      <%= link_to file_preview(material.first_file), material  %>
    </div>
  </div>
<% end %>
