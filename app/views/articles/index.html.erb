<div class="row">
  <h1>Security Education 101</h1>

  <div class="content description-text">
    <%= managed_content("articles-intro") %>
  </div>

  <% cache(cache_key_for_articles(@sections)) do %>
    <% @sections.each do |section, articles| %>
      <h2><%= section.try(:name) || "Other Articles" %></h2>
      <%= render articles %>
    <% end %>
  <% end %>
</div>
